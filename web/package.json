        "eslint": "^8.2.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "husky": "^8.0.3",
        "lint-staged": "^15.1.0",
        "prettier": "^3.1.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.0",
        "vitest": "^1.1.0",
        "vitest-fetch-mock": "^0.2.2"
