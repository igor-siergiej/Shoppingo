        "@mui/styled-engine-sc": "^6.0.0-alpha.2",
        "@mui/styles": "^5.14.20",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.58",
        "@types/react": "^18.2.25",
        "@types/react-dom": "^18.2.11",
        "jsdom": "^23.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-query": "^3.39.3",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
